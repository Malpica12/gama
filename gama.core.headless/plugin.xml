<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="id4"
         name="Headless Gama"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="gama.core.headless.runtime.Application">
         </run>
      </application>
   </extension>
   <extension
         point="gaml.extension">
   </extension>
   <extension
         point="gama.ui.provider">
      <implementation
            class="gama.core.headless.HeadlessUI"
            keyword="headless">
      </implementation>
   </extension>

</plugin>
