<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="gama.ui.viewers.image.ImageViewer"
            default="false"
            extensions="JPG,jpg,JPEG,jpeg,PNG,png,BMP,bmp,TIFF,tiff,TIF,tif,ICO,ico,ASC,asc,PGM,pgm"
            icon="platform:/plugin/gama.ui.base/icons/navigator/file.image2.png"
            id="gama.viewer.image"
            name="Image Viewer">
         <contentTypeBinding
               contentTypeId="msi.gama.gui.images.type">
         </contentTypeBinding>
      </editor>
        <editor
            class="gama.ui.viewers.csv.MultiPageCSVEditor"
            default="true"
            extensions="CSV,csv"
            icon="platform:/plugin/gama.ui.base/icons/navigator/file.csv2.png"
            id="gama.viewer.csv"
            name="CSV Editor">
         <contentTypeBinding
               contentTypeId="msi.gama.gui.csv.type">
         </contentTypeBinding>
      </editor>
      <editor
            class="gama.ui.viewers.html.HtmlViewer"
            default="false"
            extensions="html, htm, HTML, HTM, svg, SVG"
            icon="platform:/plugin/gama.ui.base/icons/navigator/file.html2.png"
            id="gama.viewer.browser"
            name="HTML Viewer">
         <contentTypeBinding
               contentTypeId="msi.gama.gui.html.type">
         </contentTypeBinding>
      </editor>
      <editor
            class="gama.ui.viewers.gis.ShapeFileViewer"
            default="true"
            extensions="shp"
            icon="platform:/plugin/gama.ui.base/icons/navigator/file.shape2.png"
            id="gama.viewer.shape"
            name="ShapeFile Viewer">
         <contentTypeBinding
               contentTypeId="msi.gama.gui.shapefile.type">
         </contentTypeBinding>
      </editor>
      <editor
            class="gama.ui.viewers.gis.OSMFileViewer"
            default="true"
            extensions="gz,osm,pbf,bz2"
            icon="platform:/plugin/gama.ui.base/icons/navigator/file.osm2.png"
            id="gama.viewer.osm"
            name="OSM Viewer">
         <contentTypeBinding
               contentTypeId="msi.gama.gui.osm.type">
         </contentTypeBinding>
      </editor>
   </extension>

</plugin>
