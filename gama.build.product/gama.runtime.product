<product name="Gama (runtime)" uid="gama.runtime.product" id="gama.core.application.product" application="gama.core.application.GamaApplication" version="2.0.0.qualifier" useFeatures="true" includeLaunchers="true">

   <aboutInfo>
      <image path="branding_icons/icon128.png"/>
      <text>
         GAMA Platform - V2.0.0 - http://gama-platform.org

